<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="wrap_content">
	
	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/ver_33"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.45" />
	
	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/ver_66"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.7"/>
	
	<TextView
		android:id="@+id/tv_item"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:fontFamily="@font/century_gothic"
		android:padding="5dp"
		android:text="DEBUG"
		android:textAlignment="center"
		android:textColor="@color/black"
		android:textSize="16sp"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toLeftOf="@id/ver_33"
		app:layout_constraintTop_toTopOf="parent"/>
	
	<TextView
		android:id="@+id/tv_quant"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:fontFamily="@font/century_gothic"
		android:padding="5dp"
		android:text="DEBUG"
		android:textAlignment="center"
		android:textColor="@color/black"
		android:textSize="16sp"
		app:layout_constraintLeft_toRightOf="@id/ver_33"
		app:layout_constraintRight_toLeftOf="@id/ver_66"
		app:layout_constraintTop_toTopOf="parent"/>
	
	<TextView
		android:id="@+id/tv_price"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:fontFamily="@font/century_gothic"
		android:hint="TBD"
		android:padding="5dp"
		android:textAlignment="center"
		android:textColor="@color/black"
		android:textSize="16sp"
		app:layout_constraintLeft_toRightOf="@id/ver_66"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>